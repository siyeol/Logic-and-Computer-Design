/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=a, b=b, out=x);
    Nand(a=a, b=x, out=p);
    Nand(a=b, b=x, out=q);
    Nand(a=p, b=q, out=out);
}